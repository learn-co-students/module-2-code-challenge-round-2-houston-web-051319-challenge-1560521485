<h1> create new guest</h1>

<%= form_with model:  @guest do |f| %>
  <div>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </div>
  <div>
  <%= f.label :occupation %>
  <%= f.text_field :occupation%>
  </div>
  <div>
  <%= f.label :episode_id %>
  <%= f.text_field :episode_id %>
  </div>
  <%= f.submit %>


  <% end %>
  